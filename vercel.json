{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "rewrites": [
    {
      "source": "/anti.html",
      "destination": "/anti.html"
    },
    {
      "source": "/daftar.html",
      "destination": "/daftar.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
